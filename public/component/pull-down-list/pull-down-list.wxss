.background-container{
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0, 0.0);
}
.shade-container{
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: rgba(0,0,0, 0.6);
}
.list-view{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: white;
  border-bottom: 1rpx solid #dcdcdc;
  text-align: center;
}
.list-container{
  border-bottom-left-radius: 15rpx;
  border-bottom-right-radius: 15rpx;
}