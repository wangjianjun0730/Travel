.navigationbar-container{
  width: 100%;
  height: 128rpx;
  background-color: gold;
  justify-content:center;
  position: relative;
  text-align: center;
}

/* 适配iPhonex */
.navigationbar-container-iphonex{
  width: 100%;
  height: 176rpx;
  background-color: gold;
  justify-content:center;
  position: relative;
}

.navigation-item-container{
  width: 100%;
  height:88rpx;
  left: 0;
  position: absolute;
  bottom: 0;
}

.left-items-container{
  width: 30%;
  height: 100%;
  /* background-color: goldenrod; */
  display:flex;
  justify-content:center;
}

/*导航栏返回按钮*/
.left-back-icon{
  width: 35rpx;
  height: 35rpx;
  margin-left: 20rpx;
  
  /*垂直居中*/
  margin:auto auto auto 20rpx;
}

/*导航栏中间title*/
.bar-title-text{
  font-size: 34rpx;
  width: 40%;
  height: 100%;
  text-align: center;
  line-height: 88rpx;
  
  /* 居中 */
  position:absolute;
  margin: auto;
  left:0;
  right:0;
  top:0;
  bottom:0;
  
}